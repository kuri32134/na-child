<h1>全ユーザー一覧</h1>
<h2>ユーザー一覧</h2>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.full_name %> - <%= user.email %>
      <%= link_to '削除', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %>
    </li>
  <% end %>
</ul>
<h2>リーダー一覧</h2>
<ul>
  <% @leaders.each do |leader| %>
    <li>
      <%= leader.name %> - <%= leader.email %>
      <%= link_to '削除', admin_leader_path(leader), method: :delete, data: { confirm: '本当に削除しますか' } %>
    </li>
  <% end %>
</ul>